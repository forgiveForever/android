<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_centerHorizontal="true"
    android:background="#90FFFFFF"
    android:gravity="bottom|center_horizontal" >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:orientation="vertical">
       <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="80dp"
           android:orientation="horizontal">
           <RelativeLayout
           android:layout_width="match_parent"
           android:layout_height="match_parent"
           android:layout_weight="1">
            <ImageView
                android:id="@+id/send_wenzi"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:src="@drawable/tabbar_compose_idea"
                android:layout_centerHorizontal="true" />
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:textSize="12dp"
                   android:text="文字"
                   android:layout_centerHorizontal="true"
                   android:layout_alignParentBottom="true"
                   android:gravity="bottom|center_horizontal"/>
       </RelativeLayout>
           <RelativeLayout
               android:layout_width="match_parent"
               android:layout_height="match_parent"
               android:layout_weight="1">
               <ImageView
                   android:id="@+id/send_xiangce"
                   android:layout_width="60dp"
                   android:layout_height="60dp"
                   android:src="@drawable/tabbar_compose_photo"
                   android:layout_centerHorizontal="true" />
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:textSize="12dp"
                   android:text="相册"
                   android:layout_centerHorizontal="true"
                   android:layout_alignParentBottom="true"
                   android:gravity="bottom|center_horizontal"/>
           </RelativeLayout>
           <RelativeLayout
               android:layout_width="match_parent"
               android:layout_height="match_parent"
               android:layout_weight="1">
               <ImageView
                   android:id="@+id/send_paishe"
                   android:layout_width="60dp"
                   android:layout_height="60dp"
                   android:src="@drawable/tabbar_compose_camera"
                   android:layout_alignParentTop="true"
                   android:layout_centerHorizontal="true" />
               <TextView
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:textSize="12dp"
                   android:text="拍摄"
                   android:layout_centerHorizontal="true"
                   android:layout_alignParentBottom="true"
                   android:gravity="bottom|center_horizontal"/>
           </RelativeLayout>
       </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:orientation="horizontal">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/send_weizi"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:src="@drawable/tabbar_compose_lbs"
                    android:layout_centerHorizontal="true" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="12dp"
                    android:text="签到"
                    android:layout_centerHorizontal="true"
                    android:layout_alignParentBottom="true"
                    android:gravity="bottom|center_horizontal"/>
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/send_dianping"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:src="@drawable/tabbar_compose_review"
                    android:layout_above="@+id/textView2"
                    android:layout_centerHorizontal="true" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="12dp"
                    android:text="点评"
                    android:layout_centerHorizontal="true"
                    android:layout_alignParentBottom="true"
                    android:gravity="bottom|center_horizontal"
                    android:id="@+id/textView2" />
            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1">
                <ImageView
                    android:id="@+id/send_gengduo"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:src="@drawable/tabbar_compose_more"
                    android:layout_centerHorizontal="true" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="12dp"
                    android:text="更多"
                    android:layout_centerHorizontal="true"
                    android:layout_alignParentBottom="true"
                    android:gravity="bottom|center_horizontal"/>
            </RelativeLayout>
        </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
          <!--  <ImageView
                android:id="@+id/center_music_window_close"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginBottom="78dip"
                android:src="@drawable/tabbar_compose_background_icon_close" />-->
        </RelativeLayout>
    </LinearLayout>
